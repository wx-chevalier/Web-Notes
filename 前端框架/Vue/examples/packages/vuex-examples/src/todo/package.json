{
	"name": "vuex-examples",
	"description": "A Vue.js project",
	"version": "1.0.0",
	"author": "Ademola Adegbuyi <ooade96@gmail.com>",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
		"build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
		"start": "superstatic /dist -p 3000 --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
		"push-surge": "npm run -s build && surge ./ -d vuex-todo.surge.sh"
	},
	"dependencies": {
		"superstatic": "^6.0.3",
		"vue": "^2.5.17",
		"vuex": "^2.5.0"
	},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"babel-loader": "^8.0.4",
		"cross-env": "^3.0.0",
		"css-loader": "^1.0.0",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"vue-loader": "^15.4.2",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.5.17",
		"webpack": "^4.20.2",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.9"
	}
}
